{
  "name": "zan/doctrine-rest-bundle",
  "description": "REST API with doctrine integrations to enable working with large result sets",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
      "php": ">=8.0",
      "doctrine/dbal": "^3.3",
      "symfony/http-foundation": "^5.4||^6.4",
      "symfony/http-kernel": "^5.4||^6.4",
      "symfony/dependency-injection": "^5.4||^6.4",
      "zan/common-bundle": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Zan\\DoctrineRestBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zan\\DoctrineRestBundle\\Tests\\": "tests/"
    }
  }
}